<div>
<h2>Tabla de productos</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Titulo</th>
      <th scope="col">Precio</th>
      <th scope="col">Miniatura</th>
    </tr>

  </thead>
  <tbody>
    
       {{#if productos.length}}
        {{#each productos}}
    <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img height="72px" width="72px" src={{this.thumbnail}} /></td>
    </tr>
{{/each}}
{{/if}}
  </tbody>
</table>
 <a href="/"><button>Volver al formulario de carga</button></a>
</div>
